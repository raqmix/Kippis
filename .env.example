APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

# PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"
# CORS Configuration
CORS_ALLOWED_ORIGINS_PATTERNS=

# JWT Configuration
JWT_ALGORITHM=HS256
JWT_SECRET=
JWT_TTL=null
# JWT_TTL: Token time-to-live in minutes. Set to null for never-expiring tokens (recommended for mobile apps)
# JWT_PUBLIC_KEY=
# JWT_PRIVATE_KEY=
# JWT_PASSPHRASE=

# Security Configuration
REQUIRE_2FA=false
ENABLE_IP_WHITELIST=false
ENABLE_IP_BLACKLIST=true
SECURITY_ALERT_EMAIL=admin@example.com

# Foodics Configuration
FOODICS_SANDBOX_CLIENT_ID=
FOODICS_SANDBOX_CLIENT_SECRET=
FOODICS_LIVE_CLIENT_ID=
FOODICS_LIVE_CLIENT_SECRET=
FOODICS_MODE=sandbox

# Social Authentication Configuration
# Google OAuth
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_REDIRECT_URI=http://localhost:8000/auth/google/callback

# Apple Sign In
APPLE_CLIENT_ID=
APPLE_CLIENT_SECRET=
APPLE_REDIRECT_URI=http://localhost:8000/auth/apple/callback
